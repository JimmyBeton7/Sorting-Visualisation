<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="SortingVisualisationFrame">
		<title>Sorting Visualisation</title>
		<size>1365,700</size>
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<pos>448,296</pos>
			<size>1365,584</size>
			<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
				<pos>16,16</pos>
				<size>1336,56</size>
				<style>wxBORDER_DOUBLE|wxTAB_TRAVERSAL</style>
				<object class="wxButton" name="ID_BUTTON1" variable="exit_btn" member="yes">
					<label>Exit</label>
					<pos>1240,8</pos>
					<size>87,40</size>
					<handler function="Onexit_btnClick" entry="EVT_BUTTON" />
				</object>
				<object class="wxChoice" name="ID_CHOICE1" variable="Choice1" member="yes">
					<content>
						<item>Bubblesort</item>
						<item>Quicksort</item>
						<item>Selectionsort</item>
						<item>Insertionsort</item>
						<item>Cocktailsort</item>
						<item>Shellsort</item>
						<item>Bingosort</item>
						<item>Cyclesort</item>
						<item>Combsort</item>
						<item>Pancakesort</item>
					</content>
					<pos>224,16</pos>
					<size>175,39</size>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>normal</weight>
						<underlined>0</underlined>
						<family>default</family>
						<face>Arial</face>
					</font>
				</object>
				<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
					<label>Choose sorting algorithm:</label>
					<pos>16,24</pos>
					<font>
						<size>11</size>
						<style>normal</style>
						<weight>normal</weight>
						<underlined>0</underlined>
						<family>default</family>
						<face>Arial</face>
					</font>
				</object>
				<object class="wxButton" name="ID_BUTTON2" variable="Button1" member="yes">
					<label>Generate</label>
					<pos>856,8</pos>
					<size>98,40</size>
					<handler function="OnButton1Click" entry="EVT_BUTTON" />
				</object>
				<object class="wxButton" name="ID_BUTTON3" variable="Button2" member="yes">
					<label>Mix</label>
					<pos>960,8</pos>
					<size>90,40</size>
					<handler function="OnButton2Click" entry="EVT_BUTTON" />
				</object>
				<object class="wxSlider" name="ID_SLIDER1" variable="Slider1" member="yes">
					<value>100</value>
					<min>10</min>
					<max>1000</max>
					<tickfreq>10</tickfreq>
					<pos>560,16</pos>
					<size>174,28</size>
				</object>
				<object class="wxButton" name="ID_BUTTON4" variable="Button3" member="yes">
					<label>Start</label>
					<pos>1056,8</pos>
					<size>88,40</size>
					<enabled>0</enabled>
					<handler function="OnButton3Click" entry="EVT_BUTTON" />
				</object>
				<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
					<label>Animation speed:</label>
					<pos>416,24</pos>
					<size>144,22</size>
					<font>
						<size>11</size>
						<style>normal</style>
						<weight>normal</weight>
						<underlined>0</underlined>
						<family>default</family>
						<face>Arial</face>
					</font>
				</object>
				<object class="wxButton" name="ID_BUTTON5" variable="Button4" member="yes">
					<label>Stop</label>
					<pos>1152,8</pos>
					<size>85,40</size>
					<enabled>0</enabled>
					<handler function="OnButton4Click" entry="EVT_BUTTON" />
				</object>
				<object class="wxButton" name="ID_BUTTON6" variable="Button5" member="yes">
					<label>Reset</label>
					<pos>760,8</pos>
					<size>93,40</size>
					<enabled>0</enabled>
					<handler function="OnButton5Click" entry="EVT_BUTTON" />
				</object>
			</object>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="Timer1" member="yes">
			<interval>100</interval>
			<handler function="OnTimer1Trigger" entry="EVT_TIMER" />
		</object>
	</object>
</wxsmith>
